{
  "manifest_version": 3,
  "name": "星函标签页",
  "version": "1.2.12",
  "description": "一个简洁而实用的新标签页扩展插件",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["tabs"],
  "icons": {
    "16": "./icons/icon16.ico",
    "48": "./icons/icon48.ico",
    "128": "./icons/icon128.ico"
  },
  "action": {
    "default_icon": {
      "16": "./icons/icon16.ico",
      "48": "./icons/icon48.ico"
    },
    "default_title": "星函标签页"
  },
  "background": {
    "service_worker": "background.js"
  }
}